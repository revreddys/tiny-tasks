{
  "expo": {
    "name": "Tiny Tasks",
    "slug": "tiny-tasks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "tinytasks",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6B9D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tinytasks.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FF6B9D"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tinytasks.app",
      "permissions": [
        "INTERNET",
        "VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FF6B9D",
          "image": "./assets/splash-icon.png",
          "imageWidth": 200
        }
      ]
    ],
    "owner": "revreddys",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "727f1e96-f0e2-4c89-aace-2ea9611a01eb"
      }
    }
  }
}
